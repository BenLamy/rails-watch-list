<div class="container">
<h1 class="d-flex justify-content-center">My Lists </h1>

 <div class="cards">
  <% @lists.each do |list| %>
    <div class="card-category">
      <%= link_to list.name, list_path(list), class:"btn btn-warning mb-4" %>
    </div>
  <% end %>
 </div>
</div>

<div class="d-flex justify-content-center p-30">
    <%= link_to "New list", new_list_path, class: 'btn btn-primary' %>
</div>
